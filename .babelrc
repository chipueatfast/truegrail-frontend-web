{
    "presets": [
        [
            "@babel/preset-env",
            {
              "modules": false,
              "useBuiltIns": "usage"
            }
        ],
        "@babel/preset-react",
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties",
        [
            "babel-plugin-root-import", {
                "rootPathPrefix": "@",
                "rootPathSuffix": "./src",
            }
        ],
        ["module-resolver", {
            "root": ["./src/"],
            "alias": {
              "@": "./src",
            }
        }]
    ],
}